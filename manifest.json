{
  "manifest_version": 3,
  "name": "Dark Moodle Concordia",
  "description": "Dark theme for the Moodle learning platform @ Concordia University.",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "css": [ "./style.css" ],
      "matches": [ "https://moodle.concordia.ca/*", "https://fas.concordia.ca/adfs/*" ]
    }
  ]
}
